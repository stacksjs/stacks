{
  "name": "vue-component-library-starter",
  "version": "0.1.0",
  "description": "Vue 3 Component Library Starter kit.",
  "keywords": [
    "component",
    "library",
    "vue",
    "typescript",
    "javascript",
    "starter",
    "kit"
  ],
  "license": "MIT",
  "author": "Meema Labs",
  "main": "./dist/my-awesome-vue-component-lib.umd.js",
  "module": "./dist/my-awesome-vue-component-lib.es.js",
  "exports": {
    ".": {
      "import": "./dist/my-awesome-vue-component-lib.es.js",
      "require": "./dist/my-awesome-vue-component-lib.umd.js"
    },
    "./styles.css": "./styles.css",
    "./postcss.config.js": "./postcss.config.js"
  },
  "files": [
    "dist",
    "src",
    "styles.css",
    "postcss.config.js"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "vitebook:dev": "vitebook dev",
    "vitebook:build": "vitebook build",
    "vitebook:preview": "vitebook preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:e2e": "cypress open",
    "coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.21.1",
    "@babel/types": "7.17.0",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "@vitebook/client": "0.23.6",
    "@vitebook/core": "0.23.6",
    "@vitebook/markdown-vue": "0.23.6",
    "@vitebook/theme-default": "0.23.6",
    "@vitebook/vue": "0.23.6",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/compiler-sfc": "3.2.33",
    "@vueuse/core": "8.3.1",
    "autoprefixer": "10.4.5",
    "eslint": "8.14.0",
    "eslint-plugin-cypress": "2.12.1",
    "pnpm": "6.32.10",
    "postcss": "8.4.12",
    "postcss-import": "14.1.0",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.10",
    "svelte": "3.47.0",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3",
    "unplugin-auto-import": "0.7.1",
    "unplugin-vue-components": "0.19.3",
    "vite": "2.9.5",
    "vitest": "0.10.0",
    "vue-tsc": "0.34.10"
  },
  "engines": {
    "node": ">=16.*"
  },
  "contributors": [
    "Chris Breuer <chris@meema.xyz>"
  ]
}