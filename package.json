    "functions",
    "ssr",
    "pwa",
    "library",
    "vue",
    "web components",
    "typescript",
    "javascript",
    "starter kit"
  ],
  "sideEffects": false,
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "engines": {
    "node": ">=v18.10.0",
    "pnpm": ">=7.13.3"
  },
  "scripts": {
