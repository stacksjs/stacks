{
  "name": "@stacksjs/framework",
  "type": "module",
  "version": "0.35.0",
  "packageManager": "pnpm@7.13.4",
  "description": "The Stacks framework core.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/.stacks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./.stacks"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "framework",
    "typescript",
    "javascript",
    "vite",
    "nitro",
    "vue",
    "functions",
    "components",
    "library",
    "starter",
    "kit"
  ],
  "contributors": [
    "Chris Breuer <chris@ow3.org>"
  ],
  "files": [
    "artisan",
    "core",
    "dist",
    "examples",
    "functions",
    "scripts",
    "vue-components",
    "web-components",
    ".gitignore",
    "custom-elements.json",
    "nitro.config.ts",
    "package.json",
    ".eslintrc-auto-import.json",
    "build.config.ts",
    "vitest.config.ts",
    "README.md",
    "shims.d.ts",
    "auto-imports.d.ts",
    "components.d.ts"
  ],
  "engines": {
    "node": ">=v18.10.0",
    "pnpm": ">=7.13.4"
  },
  "scripts": {
    "preinstall": "only-allow pnpm",
    "artisan": "jiti ./artisan/src/index.ts",
    "dev:components": "vite serve ./core/components -c ./core/build/vue-components.ts",
    "build:components": "vite build -c ./core/build/vue-components.ts",
    "build:web-components": "vite build -c ./core/build/web-components.ts",
    "build:functions": "vite build -c ./core/build/functions.ts",
    "build:pages": "vite-ssg build -c ./core/build/pages.ts",
    "build:cli": "cd ./artisan && unbuild",
    "build:stacks": "wip",
    "build": "pnpm run test:types && pnpm artisan build",
    "example:vue": "pnpm run build:components && cd examples/vue-components && ln -sf ../../vue-components/dist dist && vite serve -c ../../core/build/example-vue.ts",
    "example:web-components": "pnpm run build:web-components && cd examples/web-components && ln -sf ../../web-components/dist dist && vite serve -c ../../core/build/example-wc.ts",
    "nitro:build": "nitro build",
    "make:component": "pnpm artisan make:component",
    "make:function": "pnpm artisan make:function",
    "make:lang": "pnpm artisan make:lang",
    "fresh": "pnpm run clean && pnpm i",
    "make:stack": "pnpm artisan make:stack",
    "update": "pnpm update",
    "clean": "rimraf ./node_modules ../apps/**/node_modules ../node_modules/ ../pnpm-lock.yaml",
    "dev:docs": "vitepress dev ../apps/site",
    "build:docs": "vitepress build ../apps/site",
    "dev:pages": "nitro dev",
    "server:functions": "wip",
    "server:pages": "nitro build",
    "lint": "eslint ..",
    "lint:fix": "eslint .. --fix",
    "commit": "git cz",
    "release": "jiti ./core/actions/generate-package-json.ts && bumpp ./package.json ./**/package.json ../package.json --execute 'pnpm run changelog' --all",
    "changelog": "cd .. && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "size": "esno core/actions/size.ts",
    "test": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.config.ts",
    "test:types": "vue-tsc --noEmit",
    "types:fix": "esno core/actions/fix-types.ts",
    "generate:types": "vue-tsc --declaration --emitDeclarationOnly && jiti core/actions/copy-files.ts",
    "generate:entries": "jiti core/actions/generate-lib-entries.ts",
    "generate:vue-compatibility": "jiti core/actions/generate-vue-compat.ts",
    "generate:web-types": "vue-docgen-web-types -c ./core/web-types.config.cjs",
    "generate:vscode-custom-data": "jiti core/actions/generate-vscode-custom-data.ts",
    "generate:ide-helpers": "jiti core/actions/generate-ide-helpers.ts",
    "key:generate": "pnpm artisan key:generate"
  },
  "devDependencies": {
    "@babel/types": "^7.19.4",
    "@iconify-json/heroicons-outline": "^1.1.4",
    "@iconify-json/heroicons-solid": "^1.1.5",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@jsdevtools/ez-spawn": "^3.0.4",
    "@julr/unocss-preset-forms": "^0.0.2",
    "@ow3/eslint-config": "^0.30.2",
    "@rollup/pluginutils": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^18.8.0",
    "@types/nprogress": "^0.2.0",
    "@types/prompts": "^2.4.1",
    "@unocss/preset-icons": "^0.45.29",
    "@unocss/transformer-compile-class": "^0.45.29",
    "@unocss/vite": "^0.45.29",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitest/coverage-istanbul": "^0.24.1",
    "@vueuse/core": "^9.3.0",
    "@vueuse/head": "^0.9.8",
    "@vueuse/shared": "^9.3.0",
    "artisan-init": "workspace:*",
    "bcryptjs": "^2.4.3",
    "bumpp": "^8.2.1",
    "cac": "^6.7.14",
    "change-case": "^4.1.2",
    "collect.js": "^4.34.3",
    "commitizen": "^4.2.5",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.2.2",
    "crypto-js": "^4.1.1",
    "cz-git": "^1.3.11",
    "detect-indent": "^7.0.1",
    "detect-newline": "^4.0.0",
    "eslint": "^8.25.0",
    "esno": "^0.16.3",
    "events": "^3.3.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "giget": "^0.1.7",
    "jiti": "^1.16.0",
    "js-base64": "^3.7.2",
    "lint-staged": "^13.0.3",
    "local-pkg": "^0.4.2",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.5.1",
    "minimatch": "^5.1.0",
    "nitropack": "^0.5.4",
    "nprogress": "^0.2.0",
    "only-allow": "^1.1.1",
    "pathe": "^0.3.9",
    "pinia": "^2.0.23",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.0",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "unocss": "^0.45.29",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.7",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-vue-component-preview": "^0.3.3",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-markdown": "^0.22.1",
    "vite-ssg": "^0.21.1",
    "vite-ssg-sitemap": "^0.4.3",
    "vitepress": "1.0.0-alpha.19",
    "vitest": "^0.24.1",
    "vue": "^3.2.40",
    "vue-docgen-web-types": "^0.1.8",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "vue-tsc": "^1.0.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "web-types": "./web-types.json"
}
