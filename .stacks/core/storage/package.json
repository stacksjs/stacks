    "@types/archiver": "^5.3.4",
    "archiver": "^6.0.1",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "unstorage": "^1.9.0"
  },
  "devDependencies": {
    "@stacksjs/development": "workspace:*"
  }
