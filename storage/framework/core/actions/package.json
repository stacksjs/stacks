{
  "name": "@stacksjs/actions",
  "type": "module",
  "version": "0.70.23",
  "description": "The Stacks actions.",
  "author": "Chris Breuer",
  "contributors": [
    "Chris Breuer <chris@stacksjs.com>"
  ],
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/storage/framework/core/actions#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./storage/framework/core/actions"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "actions",
    "stacks",
    "framework",
    "typescript",
    "javascript",
    "bun"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./run-action": {
      "bun": "./src/run-action.ts",
      "types": "./dist/run-action.d.ts",
      "import": "./dist/run-action.js"
    },
    "./*": {
      "bun": "./src/*",
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "bun build.ts",
    "typecheck": "bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@stacksjs/ts-cloud": "^0.1.11"
  },
  "devDependencies": {
    "@stacksjs/api": "workspace:*",
    "@stacksjs/cli": "workspace:*",
    "@stacksjs/config": "workspace:*",
    "@stacksjs/database": "workspace:*",
    "@stacksjs/tlsx": "^0.13.0",
    "better-dx": "^0.2.5",
    "@stacksjs/dns": "workspace:*",
    "@stacksjs/enums": "workspace:*",
    "@stacksjs/env": "workspace:*",
    "@stacksjs/error-handling": "workspace:*",
    "@stacksjs/logging": "workspace:*",
    "@stacksjs/path": "workspace:*",
    "@stacksjs/security": "workspace:*",
    "@stacksjs/storage": "workspace:*",
    "@stacksjs/strings": "workspace:*",
    "@stacksjs/utils": "workspace:*",
    "@stacksjs/validation": "workspace:*"
  }
}
