  "scripts": {
    "build": "bun build.ts",
    "typecheck": "bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@stripe/stripe-js": "^8.8.0",
    "bun-query-builder": "^0.1.13",
