{
  "name": "@stacksjs/ai",
  "type": "module",
  "version": "0.64.6",
  "description": "Stacks Artificial Intelligence.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
