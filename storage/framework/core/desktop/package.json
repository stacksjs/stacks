{
  "name": "@stacksjs/desktop",
  "type": "module",
  "version": "0.70.23",
  "description": "The Stacks Desktop engine.",
  "author": "Chris Breuer",
  "contributors": [
    "Chris Breuer <chris@stacksjs.com>"
  ],
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/storage/framework/core/desktop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./storage/framework/core/desktop"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": [
    "desktop",
    "nexmo",
    "twilio",
    "aws",
    "sms",
    "email",
    "database",
    "slack",
    "discord",
    "webhooks",
    "stacks"
  ],
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "bun": "./src/*",
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "dev": "bunx --bun vite ../../dashboard -c ../vite-config/src/desktop.ts",
    "dev:app": "tauri dev",
    "build": "bun build.ts",
    "build:app": "bunx --bun vite build ../../dashboard -c ../vite-config/src/desktop.ts",
    "tauri:build": "tauri build",
    "typecheck": "bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@stacksjs/build": "workspace:*",
    "better-dx": "^0.2.5",
    "@stacksjs/server": "workspace:*",
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "unified-network": "^0.6.4"
  }
}
