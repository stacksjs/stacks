{
  "name": "@stacksjs/docs",
  "type": "module",
  "version": "0.70.23",
  "description": "The Stacks way to document.",
  "author": "Chris Breuer",
  "contributors": ["Chris Breuer <chris@stacksjs.org>"],
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/storage/framework/core/docs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/stacks.git",
    "directory": "./storage/framework/core/docs"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/stacks/issues"
  },
  "keywords": ["docs", "utilities", "functions", "stacks"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["README.md", "dist"],
  "scripts": {
    "build": "bun build.ts",
    "typecheck": "bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@iconify-json/carbon": "^1.2.9",
    "@shikijs/vitepress-twoslash": "^3.6.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "@vite-pwa/vitepress": "^1.0.0",
    "unocss": "^66.1.4",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.7.0",
    "vite-plugin-pwa": "^1.0.0",
    "vitepress": "1.6.3"
  },
  "devDependencies": {
    "@stacksjs/alias": "workspace:*",
    "@stacksjs/config": "workspace:*",
    "@stacksjs/development": "workspace:*",
    "@stacksjs/path": "workspace:*",
    "@stacksjs/server": "workspace:*"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
