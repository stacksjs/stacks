{
  "name": "@stacksjs/security",
  "type": "module",
  "version": "0.61.0",
  "description": "The Stacks framework security.",
  "author": "Chris Breuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/chrisbbreuer",
  "homepage": "https://github.com/stacksjs/stacks/tree/main/storage/framework/core/security#readme",
