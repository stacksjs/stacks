    "@vueuse/head": "^2.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "vite-plugin-layouts": "^0.1.10",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.2.10",
