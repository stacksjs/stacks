{
  "name": "stacks-web",
  "type": "module",
  "version": "0.70.23",
  "private": true,
  "scripts": {
    "build": "bun --bun vite-ssg build -c ../../core/vite-config/src/views.ts",
    "dev": "bun --bun vite serve -c ../../core/vite-config/src/views.ts",
    "preview": "bun --bun vite preview",
