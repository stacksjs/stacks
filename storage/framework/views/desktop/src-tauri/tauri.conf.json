{
  "build": {
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun build:app",
    "devUrl": "http://localhost:3004/",
    "frontendDist": "../../dashboard/dist"
  },
  "identifier": "com.stacks.localhost",
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "capabilities": ["main-capability"]
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Stacks App",
        "width": 800
      }
    ]
  }
}
